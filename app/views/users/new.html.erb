<div>
  <h1>Sign up for Filmr!</h1>
  <%= form_for @user do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
    <div>
      <%= form.submit "Sign up" %>
    </div>
  <% end %>
</div>
